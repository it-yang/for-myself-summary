<script defer="defer">
	
	$(document).ajaxStart(function() {
		$('#spinner').show();
	}).ajaxStop(function() {
		$('#spinner').hide();
	});

	var timeout = 500;
	var closetimer = 0;
	var ddmenuitem = 0;

	function jsddm_open() {
		jsddm_canceltimer();
		jsddm_close();
		ddmenuitem = $(this).find('ul').eq(0).css('visibility', 'visible');
	}

	function jsddm_close() {
		if(ddmenuitem){
			ddmenuitem.css('visibility', 'hidden');
		}
	}

	function jsddm_timer() {
		closetimer = window.setTimeout(jsddm_close, timeout);
	}

	function jsddm_canceltimer() {
		if(closetimer) {
			window.clearTimeout(closetimer);
			closetimer = null;
		}
	}

	$(document).ready(function() {
		$('#jsddm > li').bind('mouseover', jsddm_open);
		$('#jsddm > li').bind('mouseout', jsddm_timer);
	});

	document.onclick = jsddm_close;

	function setTab03Syn(i) {
		selectTab03Syn(i);
	}

	function selectTab03Syn(i) {
		switch(i) {
			case 1:
				document.getElementById("TabTab03Con1").style.display = "block";
				document.getElementById("TabTab03Con2").style.display = "none";
				document.getElementById("font1").style.color = "#ffffff";
				document.getElementById("font2").style.color = "#666666";
				break;
			case 2:
				document.getElementById("TabTab03Con1").style.display = "none";
				document.getElementById("TabTab03Con2").style.display = "block";
				document.getElementById("font1").style.color = "#666666";
				document.getElementById("font2").style.color = "#ffffff";
				break;
		}
	}
	
	window.myobj = {
		markerOver : function() {
			this.openInfoWindow();
		},
		markerOut : function() {
			this.closeInfoWindow();
		}
	};

	function loadReviews_relate(){
		url = '/org/getSumscore/{{org.id}}/';
		$.ajax({
			 url: url,
			 data: {},
			 success: function(data){
				$('#sum_stars').html('<span title=""><img style="float: left;height: 13px;margin-right: 7px;margin-top: 4px;width: 64px;" src="{{STATIC_URL}}img/p_'+data['star']+'.jpg" /></span><a  href="/org/{{org.id}}/review/" class="course_zhe">'+data['votes']+'封点评</a> | <span class="cankao">参考价格</span><span class="addguanzhu1">¥'+data['cost']+'</span>');
			 	$('#1_rating').html('<span class="progres-list2" ><span class="power" style="width:'+data['1_rating_bar']+'%"></span></span><a class="dianpnum1" href="#">'+data['1_rating']+'封</a></span>');
			 	$('#2_rating').html('<span class="progres-list2" ><span class="power" style="width:'+data['2_rating_bar']+'%"></span></span><a class="dianpnum1" href="#">'+data['2_rating']+'封</a></span>');
			 	$('#3_rating').html('<span class="progres-list2" ><span class="power" style="width:'+data['3_rating_bar']+'%"></span></span><a class="dianpnum1" href="#">'+data['3_rating']+'封</a></span>');
			 	$('#4_rating').html('<span class="progres-list2" ><span class="power" style="width:'+data['4_rating_bar']+'%"></span></span><a class="dianpnum1" href="#">'+data['4_rating']+'封</a></span>');
			 	$('#5_rating').html('<span class="progres-list2" ><span class="power" style="width:'+data['5_rating_bar']+'%"></span></span><a class="dianpnum1" href="#">'+data['5_rating']+'封</a></span>');
			 	$('#environment_total').html('<span class="progres-list2"> <span class="power" style="width:'+data['environment_total_bar']+'%"></span></span><a class="nite" href="#">'+data['environment_total']+'</a>');
			 	$('#teacher_total').html('<span class="progres-list2"> <span class="power" style="width:'+data['teacher_total_bar']+'%"></span></span><a class="nite" href="#">'+data['teacher_total']+'</a>');
			 	$('#enjoy_total').html('<span class="progres-list2"> <span class="power" style="width:'+data['enjoy_total_bar']+'%"></span></span><a class="nite" href="#">'+data['enjoy_total']+'</a>');
			 }
		});
	}
	
	function loadReviews() {
		url = '/org/getReviews/{{org.id}}/';
		data = {
			sort : $('#ReviewSort').val()
		};
		$.ajax({ url: url,
				 data: data,
				 success: function(data){
				    $('#reviewList').html(data);
				    initialReplyComment();
				 }
			  });
	}

	function initRating() {
		$('#star').raty({
			path : '{{ STATIC_URL }}img/',
			cancel : false,
			cancelOff : 'cancel-off-big.png',
			cancelOn : 'cancel-on-big.png',
			half : false,
			width : '300px',
			size : 24,
			starHalf : 'star-half-big.png',
			starOff : 'star-off-big.png',
			starOn : 'star-on-big.png'
		});
	}

	function clearReviewForm() {
		$($('.envscore').removeClass('active-square'));
		$($('.teascore').removeClass('active-square'));
		$($('.boyscore').removeClass('active-square'));
		$('#reviewForm')[0].reset();
		initRating();
	}

	function initialReplyComment() {
		$('a.replyComment').click(function() {
			$(this).next().toggle();
			return false;
		});
	}

	function initialTabs() {
		$('#myTab a').click(function(e) {
			e.preventDefault();
			$(this).tab('show');
		})
	}

	function loadmap() {
		map = new BMap.Map("map_canvas");
		var point = new BMap.Point("{{org.longitude}}","{{org.latitude}}");
		map.centerAndZoom(point, 16);
		var opts = {offset: new BMap.Size(0, 0)};
		map.addControl(new BMap.NavigationControl(opts)); //平移缩放控件
		marker = new BMap.Marker(point , {'title':'{{ org.address }}','enableDragging':false});
		map.addOverlay(marker);
	}

	function rating() {
		var ratingScore = $('#star input[name="score"]').val();
		data = {
			org_id : $('#org_id').val(),
			rating : ratingScore,
			comment : $('#comment').val(),
			environment : $('#environment').val(),
			teacher : $('#teacher').val(),
			enjoy : $('#enjoy').val(),
			price : $('#pricing').val(),
			cost : $('#cost').val()
		};
		$.ajax({
			type : "GET",
			url : "/org/rating",
			data : data,
			dataType : "json",
			success : function(data) {
				//alert(data.result);
				if(data.result == 'ok') {
					loadReviews();
					loadReviews_relate();
					clearReviewForm();
					alert('点评成功!');
				} else {
					alert('点评失败！- -');
				}
			},
			error : function(xhr, ajaxOptions, thrownError) {
				//alert(thrownError);
				//alert(xhr.status);
				alert('保存失败!- -');
			}
		});
	}
	
	$(document).ready(function() {
		initRating();
		initialReplyComment();
		$('#ReviewSort').on('change', function(event) {
			loadReviews();
		});

		$('#AddReviewBtn').on('click', function(event) {

			if($('#star input[name="score"]').val() == '') {
				$('#T_star').css({
					color : '#FF63B5'
				});
			} else {
				$('#T_star').css({
					color : '#333333'
				});
			}

			if($('#environment').val() == "") {
				$('#T_enviroment').css({
					color : '#FF63B5'
				});
			} else {
				$('#T_enviroment').css({
					color : '#333333'
				});
			}

			if($('#teacher').val() == "") {
				$('#T_teacher').css({
					color : '#FF63B5'
				});
			} else {
				$('#T_teacher').css({
					color : '#333333'
				});
			}

			if($('#enjoy').val() == "") {
				$('#T_enjoy').css({
					color : '#FF63B5'
				});
			} else {
				$('#T_enjoy').css({
					color : '#333333'
				});
			}

			if($('#cost').val() == "") {
				$('#T_cost').css({
					color : '#FF63B5'
				});
			} else {
				$('#T_cost').css({
					color : '#333333'
				});
			}

			if(is_user_login()) {
				if($('#star input[name="score"]').val() != '' && $('#environment').val() != '' && $('#teacher').val() != '' && $('#enjoy').val() != '' && $('#cost').val().replace(/(^\s*)|(\s*$)/g, "") != '') {
					var reg = /^-?[1-9]+(\.\d+)?$|^-?0(\.\d+)?$|^-?[1-9]+[0-9]*(\.\d+)?$/;
					if(reg.test($('#cost').val())) {
						if($('#comment').val().replace(/(^\s*)|(\s*$)/g, "").length < 8 || $('#comment').val().replace(/(^\s*)|(\s*$)/g, "").length > 250) {
							alert('您的评价字数不符合要求.当前字数:' + $('#comment').val().replace(/(^\s*)|(\s*$)/g, "").length + ',系统限制:8-250');
						} else {
							rating();
						}
					} else {
						alert('价格应为数字!');
						return false;
					}
				} else {
					//alert('带*号为必填项!');
					return false;
				}
			} else {
				//alert("用户您好，请登录后在点评！");
				showlogin();
				return false;
			}
		});
		loadmap();
		initialTabs();

	});
	function gbcount() {
		var max;
		max = 250;
		if($("#comment").val().replace(/(^\s*)|(\s*$)/g, "").length > max) {
			$("#comment").val($("#comment").val().replace(/(^\s*)|(\s*$)/g, "").substring(0, max));
			$("#used").html('max');
			$("#remain").html(0);
			alert("留言不能超过250个字!");
		} else {
			$("#used").html($("#comment").val().replace(/(^\s*)|(\s*$)/g, "").length);
			$("#remain").html(max - $("#comment").val().replace(/(^\s*)|(\s*$)/g, "").length);
		}
	}
</script>

<script defer="defer">
// 未登录预约开始  
	$('#order_dialog').dialog({
        autoOpen: false,
        height: 380,
		width: 680,
        show: "blind",
        hide: "slide"
	 });
	
	function unlogined_order_org() {
    	$('#order_dialog').dialog("open");
        return false;
	}
	
	var _tag = $('#order_username'), _editors = $('#phone_num'), _tags = $('#user_acnum'), _editor = $('#phone_number');
	var reg = /^-?[1-9]+(\.\d+)?$|^-?0(\.\d+)?$|^-?[1-9]+[0-9]*(\.\d+)?$/;
	var tab = /(^((0[1,2]{1}\d{1}-?\d{8})|(0[3-9]{1}\d{2}-?\d{7,8}))$)|(^0?(13[0-9]|15[0-35-9]|18[0236789]|14[57])[0-9]{8}$)/;
	
	_tag.blur(function() {
		var self = $(this);
		if(self.val() === "") {
			_tags.text("联系人名字不可为空.");
			if(_tags.hasClass('logincon_inputs')) {
				_tags.removeClass('logincon_inputs');
				_tags.addClass('misspsw');
			}
		} else if(self.val().length < 2 || self.val().length > 10) {
			if(_tags.hasClass('logincon_inputs')) {
				_tags.removeClass('logincon_inputs');
				_tags.addClass('misspsw');
			}
			_tags.text("请输入有效的长度（2-10）");
		} else {
			if(_tags.hasClass('misspsw')) {
				_tags.removeClass('misspsw');
				_tags.addClass('logincon_inputs');
			}
			_tags.text("输入正确");
		}
	});

	_editors.blur(function() {
		var self = $(this);
		if(self.val() === "") {
			_editor.text("请输入号码.");
			if(_editor.hasClass('logincon_inputs')) {
				_editor.removeClass('logincon_inputs');
				_editor.addClass('misspsw');
			}
		} else if(tab.test(self.val()) == false) {
			if(_editor.hasClass('logincon_inputs')) {
				_editor.removeClass('logincon_inputs');
				_editor.addClass('misspsw');
			}
			_editor.text("手机或固话不正确，请重新输入手机或固话号！");
		} else {
			if(_editor.hasClass('misspsw')) {
				_editor.removeClass('misspsw');
				_editor.addClass('logincon_inputs');
			}
			_editor.text("输入正确");
		}

	});

	function check_submit() {
		_tag.blur();
		_editors.blur();
		if(_editor.hasClass('misspsw') || _tags.hasClass('misspsw')) {
			return false;
		} else {
			$.ajax({
					type : "get",
					url : "/org/unlogin_order/",
					data : {
						'type':'org',
						'type_id':'{{org.id}}',
						'order_username' : _tag.val(),
						'phone_num' : _editors.val()
					},
					dataType : "json",
					success : function(data) {
						if(data['user_info'] == "true") {
							alert("预约成功！");
							$("#order_dialog").dialog("close");
							$('#order_button').html('<img src="{{STATIC_URL}}img/freeordered.gif">');
						} else if(data['user_info'] == "false") {
							alert("抱歉，预约失败，请重试！");
							$("#order_dialog").dialog("close");
						}
					},
					error : function() {
						$("#order_dialog").dialog("close");
					}
				});
		}
	}
// 未登录预约结束
// 已登录预约开始
	$("#logined_order_dialog").dialog({
			autoOpen : false,
			show : "explode",
			//hide: "explode",
			height : 300,
			width : 350,
			modal : true,
			buttons : {
				"取消" : function() {
					$("#logined_order_dialog").dialog("close");
				},
				"提交" : function() {
					check_phnum();
				}
			},
			close : function() {
				//allFields.val( "" ).removeClass( "ui-state-error" );
			}
	});
		
	function shouw() {
			$("#logined_order_dialog").dialog("open");
		}
		
	function logined_order_org() {
			$.ajax({
				type : "get",
				url : "/org/logined_order/",
				data : {},
				dataType : "json",
				success : function(data) {
					if(data['user_info'] == "true") {
						var tel = data['tel']
						$('#phone_num2').val(tel);
						shouw();
					} else if(data['user_info'] == "false") {
						shouw();
					} else if(data['user_info'] == "middle") {
						alert("您已预约，请勿重复预约！");
					}
				},
				error : function() {
					// alert("error");
				}
			});
		}
		
		var _tag2 = $('#order_username2'),_editors2 = $('#phone_num2'), _editor2 = $('#phone_number2');
		var reg = /^-?[1-9]+(\.\d+)?$|^-?0(\.\d+)?$|^-?[1-9]+[0-9]*(\.\d+)?$/;
		var tab = /(^((0[1,2]{1}\d{1}-?\d{8})|(0[3-9]{1}\d{2}-?\d{7,8}))$)|(^0?(13[0-9]|15[0-35-9]|18[0236789]|14[57])[0-9]{8}$)/;

		_editors2.blur(function() {
			if(_editors2.val() === "") {
				_editor2.text("请输入号码！");
				if(_editor2.hasClass('logincon_inputs')) {
					_editor2.removeClass('logincon_inputs');
					_editor2.addClass('misspsw');
				}
			} else if(tab.test(_editors2.val()) == false) {
				if(_editor2.hasClass('logincon_inputs')) {
					_editor2.removeClass('logincon_inputs');
					_editor2.addClass('misspsw');
				}
				_editor2.text("手机或固话不正确，请重新输入手机或固话号！");
			} else {
				if(_editor2.hasClass('misspsw')) {
					_editor2.removeClass('misspsw');
					_editor2.addClass('logincon_inputs');
				}
				_editor2.text("输入正确");
			}
		});

		function check_phnum() {
			if(_editor2.hasClass('misspsw')) {
				return false;
			} else {
				$.ajax({
					type : "get",
					url : "/org/dialog_order/",
					data : {
						'type':'org',
						'type_id':'{{org.id}}',
						'order_username' : _tag2.val(),
						'phone_num' : _editors2.val()
					},
					dataType : "json",
					success : function(data) {
						if(data['user_info'] == "true") {
							alert("预约成功！");
							$("#logined_order_dialog").dialog("close");
							$('#order_button').html('<img src="{{STATIC_URL}}img/freeordered.gif">');
						} else if(data['user_info'] == "false") {
							alert("抱歉，预约失败，请重试！");
							$("#logined_order_dialog").dialog("close");
						}
					},
					error : function() {
						$("#logined_order_dialog").dialog("close");
					}
				});
			}
		}
// 已登录 预约结束 
// 咨询提问
	$("#consult_and_question_dialog").dialog({
		autoOpen : false,
		show : "explode",
		//hide: "explode",
		height : 348,
		width : 415,
		modal : true,
		buttons : {
			"取消" : function() {
				$("#consult_and_question_dialog").dialog("close");
			},
			"提交" : function() {
				submit_cq();
			}
		},
		close : function() {
			//allFields.val( "" ).removeClass( "ui-state-error" );
		}
	});
	
	$("#cq_success_dialog").dialog({
		autoOpen : false,
		show : "explode",
		//hide: "explode",
		height : 348,
		width : 415,
		modal : true,
		close : function() {
			//allFields.val( "" ).removeClass( "ui-state-error" );
		}
	});
	
	function show_cq() {
		$("#consult_and_question_dialog").dialog("open");
	}
	function show_cq_success() {
		$("#consult_and_question_dialog").dialog("close");
		$("#cq_success_dialog").dialog("open");
	}
		
	function submit_cq() {
		if($('#cq_content_warns').hasClass('misspsw')||$('#cq_content').val() === ""){
			$('#cq_content_warns').text("内容不能为空！");
			if($('#cq_content_warns').hasClass('logincon_inputs')) {
				$('#cq_content_warns').removeClass('logincon_inputs');
				$('#cq_content_warns').addClass('misspsw');
			}
		}else{
				$.ajax({
					type : "get",
					url : "{% url consult_and_question org.id %}?sort=submit_cq",
					data : {'cq_contents':$('#cq_content').val(),'org':'{{org.id}}'},
					dataType : "json",
					success : function(data) {
						var cq_content = data['cq_contents'].length;
						if(data['mes']=='sub_true'){
							show_cq_success();
							if(cq_content<20){
								var cq_contents = data['cq_contents']
								$('#ajax_cq').append('<li><a href="{% url consult_and_question org.id %}?sort=cq_detail&cq_id='+data['cq_id']+'" target="_blank">'+cq_contents+'</a></li>');
								$('#cq_de').html('');
							}else{
								var cq_contents = data['cq_contents'].substring(0,20); 
								$('#ajax_cq').append('<li><a href="{% url consult_and_question org.id %}?sort=cq_detail&cq_id='+data['cq_id']+'" target="_blank">'+cq_contents+'..?</a></li>');
								$('#cq_de').html('');
							}
						}
					},
					error : function() {
						//alert("error");
					}
				});
			}
		}
		
		$('#cq_content').blur(function() {
			if($('#cq_content').val() === "") {
				$('#cq_content_warns').text("内容不能为空！");
				if($('#cq_content_warns').hasClass('logincon_inputs')) {
					$('#cq_content_warns').removeClass('logincon_inputs');
					$('#cq_content_warns').addClass('misspsw');
				}
			} else {
				if($('#cq_content_warns').hasClass('misspsw')) {
					$('#cq_content_warns').removeClass('misspsw');
					$('#cq_content_warns').addClass('logincon_inputs');
				}
				$('#cq_content_warns').text("输入正确！");
			}
		});
// 咨询提问结束  
</script>
{% endblock %}
