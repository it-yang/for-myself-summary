<script>  	
	$(document).ready(function(){
		if('{{user}}'!='AnonymousUser'){
			$.ajax({
				type : "get",
				url : "/org/is_sellect_button_tools/",
				data : {
					'user_id':{{user.id}},
					'type_id':{{org.id}},
					'type':'org',
					'button_type':'collect'
				},
				dataType : "json",
				success : function(data) {
					// alert(data['is_sellect']);
					if(data['is_sellect'] == 'true'){
						document.getElementById('user_collect').style.display="none";
						document.getElementById('user_cancle_collect').style.display="inline";
					}
				},
				error : function(){
					//alert("error!!!");
				}
			});
			
			$.ajax({
				type : "get",
				url : "/org/is_sellect_button_tools/",
				data : {
					'user_id':{{user.id}},
					'type_id':{{org.id}},
					'type':'org',
					'button_type':'enjoy'
				},
				dataType : "json",
				success : function(data) {
					// alert(data['is_sellect']);
					if(data['is_sellect'] == 'true'){
						document.getElementById('user_enjoy').style.display="none";
						document.getElementById('user_cancle_enjoy').style.display="inline";
					}
				},
				error : function(){
					//alert("error!!!");
				}
			});
		}
	});
	
	$(document).ready(function(){
		var json1={};
		if('{{user}}'!='AnonymousUser'){
			json1={'user_id':{{ user.id }}};
		}else{
			json1={'user_id':0};
		}
		$.ajax({
			type : "get",
			url : "/org/{{org.id}}/is_follow",
			data : json1,
			dataType : "json",
			success : function(data) {
			if(data['message']=='exists'){
				$('#followit').html('取消关注('+data['followers']+')').css({color:'grey'}).on('click',unfollow);
			}else{
				$('#followit').html('关注('+data['followers']+')').on('click',followit);
			}
			},
			error : function(){
			//alert("error!!!");
			}
		});
	
		var followit=function(){
		if('{{user}}'!='AnonymousUser'){
			$.ajax({
				type : "get",
				url : '/org/{{org.id}}/follow',
				data : {},
				dataType : "json",
				success : function(data) {
					$('#followit').html('取消关注('+data['followers']+')').css({color:'grey'}).off('click',followit).on('click',unfollow);
				},
				error : function(){
				// alert("error!!!");
				}
			});
		}else{
			showlogin();
		}
		}
		var unfollow=function(){
			$.ajax({
				type : "get",
				url : '/org/{{org_id}}/unfollow',
				data : {},
				dataType : "json",
				success : function(data) {
					$('#followit').html('关注('+data['followers']+')').css({color:'#0088CC'}).off('click',unfollow).on('click',followit);
				},
				error : function(){
				// alert("error!!!");
				}
			});
		}
	});
</script>
