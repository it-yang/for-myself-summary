<script defer="defer">
	$(function() {
	  document.getElementById("fontA").style.color = "#ffffff";
	  document.getElementById("fontB").style.color = "#7bab07";
	})
	
	function setTab03SynA(i) {
		selectTab03SynA(i);
	}

	function selectTab03SynA(i) {
		switch(i) {
			case 30:
				document.getElementById("agTabTab03ConA").style.display = "block";
				document.getElementById("agTabTab03ConB").style.display = "none";
				document.getElementById("fontA").style.color = "#ffffff";
				document.getElementById("fontB").style.color = "#7bab07";
				break;
			case 40:
				document.getElementById("agTabTab03ConA").style.display = "none";
				document.getElementById("agTabTab03ConB").style.display = "block";
				document.getElementById("fontA").style.color = "#7bab07";
				document.getElementById("fontB").style.color = "#ffffff";
				break;

		}
	}

	function show_view1(items) {
		$('.envscore').removeClass('active-square');
		$('#e_' + items).addClass('active-square');
		$('#environment').val(items);
	}

	function start_view1(val){
		$('.envscore').removeClass('active-square');
		$('#e_' + val).addClass('active-square');
	}
	
	function end_view1(){
		$('.envscore').removeClass('active-square');
		var obj = $('#environment').val();
		$('#e_' + obj).addClass('active-square');
	}
	
	function show_view2(items) {
		$('.teascore').removeClass('active-square');
		$('#tea_' + items).addClass('active-square');
		$('#teacher').val(items);
	}
	
	function start_view2(val){
		$('.teascore').removeClass('active-square');
		$('#tea_' + val).addClass('active-square');
	}
	
	function end_view2(){
		$('.teascore').removeClass('active-square');
		var obj = $('#teacher').val();
		$('#tea_' + obj).addClass('active-square');
	}

	function show_view3(items) {
		$('.boyscore').removeClass('active-square');
		$('#b_' + items).addClass('active-square');
		$('#enjoy').val(items);
	}
	
	function start_view3(val){
		$('.boyscore').removeClass('active-square');
		$('#b_' + val).addClass('active-square');
	}
	
	function end_view3(){
		$('.boyscore').removeClass('active-square');
		var obj = $('#enjoy').val();
		$('#b_' + obj).addClass('active-square');
	}
	
	function tabs_view(items) {
		$($('.tabs_css').removeClass('ta-set'));
		$('#tab_' + items + '').addClass('ta-set tabs_css');
	}

</script>
